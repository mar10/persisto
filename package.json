{
  "name": "persisto",
  "version": "2.0.0-0",
  "title": "Persistent JavaScript objects and web forms using Web Storage.",
  "description": "Persist JavaScript objects to localStorage and remote servers.",
  "main": "dist/persisto.umd.js",
  "types": "dist/persisto.d.ts",
  "module": "dist/perrsisto.esm.min.js",
  "homepage": "https://github.com/mar10/persisto",
  "author": {
    "name": "Martin Wendt",
    "url": "http://careers.stackoverflow.com/martin-wendt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mar10/persisto"
  },
  "bugs": {
    "url": "https://github.com/mar10/persisto/issues"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.githubusercontent.com/mar10/persisto/master/LICENSE.txt"
    }
  ],
  "exports": {
    ".": {
      "require": "./dist/persisto.umd.min.js",
      "import": "./dist/persisto.esm.min.js"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "ajax",
    "cache",
    "localStorage",
    "sessionStorage",
    "persistence"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/jest": "^26.0.15",
    "eslint": "^7.8.1",
    "eslint-config-jquery": "^3.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "grunt": "^1.1.0",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.5.0",
    "grunt-contrib-concat": "^1.0.0",
    "grunt-contrib-connect": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-qunit": "^4.0.0",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^21.0.0",
    "grunt-text-replace": "^0.4.0",
    "grunt-yabs": "^1.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.32.1",
    "terser": "^5.3.8",
    "ts-jest": "^26.4.3",
    "tslib": "^2.0.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {},
  "scripts": {
    "test": "grunt ci --verbose",
    "docs": "typedoc && touch docs/.nojekyll",
    "build:types": "tsc -t esnext --moduleResolution node -d --emitDeclarationOnly --outFile dist/persisto.d.ts src/persisto.ts",
    "build:js": "rollup -c rollup.config.js",
    "build:minjs:umd": "terser dist/persisto.umd.js --compress --mangle > dist/persisto.umd.min.js",
    "build:minjs:esm": "terser dist/persisto.esm.js --compress --mangle > dist/persisto.esm.min.js",
    "build:minjs": "npm run build:minjs:esm -s && npm run build:minjs:umd -s",
    "build": "npm run build:js -s && npm run build:minjs -s && npm run build:types -s"
  },
  "npmName": "persisto",
  "npmFileMap": [
    {
      "basePath": "dist",
      "files": [
        "persisto.js",
        "persisto.min.js",
        "persisto.min.js.map"
      ]
    }
  ]
}
